@model IEnumerable<Model.Endring>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Endring</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Tidspunkt)</th>
        </tr>
    </thead>
</table>

<div class="scrollable-table">

    <table class="table">
        
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <b>
                            @Html.DisplayFor(modelItem => item.EndringOperasjon)
                        </b>
                        @Html.Raw(HttpUtility.HtmlDecode(item.endring))
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Tidspunkt)
                    </td>
                </tr>
            }

        </tbody>

    </table>
    
</div>
<table class="table" style="margin-top: 10%">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Feil</th>

        </tr>
    </thead>
</table>